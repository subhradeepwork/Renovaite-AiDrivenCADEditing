# Copy this file to .env (remove .example) and fill in values as needed.

# OpenAI API key (required)
OPENAI_API_KEY=sk-REPLACE_ME

# Qdrant connection
QDRANT_URL=http://localhost:6333

# Your collection name in Qdrant
QDRANT_COLLECTION=architectural_objects

# Embedding model you use for vectors (1536 dims)
EMBEDDING_MODEL=text-embedding-3-small
